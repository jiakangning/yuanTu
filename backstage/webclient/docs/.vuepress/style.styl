.home .hero h1
  display none
.navbar
  z-index: 10
  .logo
    height: 1.8rem
    min-width: 1.8rem
    vertical-align: middle

::-webkit-scrollbar
  width: 5px
  height: 5px

::-webkit-scrollbar
  background-color: #f5f5f5

::-webkit-scrollbar-thumb
  background-color: rgba(0, 0, 0, 0.2)

::-webkit-scrollbar-thumb:hover
  background-color: rgba(0, 0, 0, 0.5)

::-webkit-scrollbar-track
  -webkit-box-shadow: inset 0 0 1px rgba(0, 0, 0, 0)
  border-radius: 10px
  background-color: transparent

.el-table__header
  margin: 0

.el-table__body
  margin: 0
  display: table

.el-card__header
  padding: 8px 20px

.el-table
  th, td
    padding: 6px 0

.example-page
  .content
    max-width: 1500px
    .mmbsWork
      width: 100%
      margin: 0
  .custom-block
    margin-left: 15px
    margin-right: 15px

.markdown-page
  h1,h2,h3,h4,h5,h6
    border none
  .content code
    line-height 30px
  table
    margin 1rem 0
    display: block

  tr
    border-top 1px solid #dfe2e5
    background-color #ffffff
    &.title
      background-color #fafafa

  th, td
    border 1px solid #dfe2e5
    padding .6em 1em
